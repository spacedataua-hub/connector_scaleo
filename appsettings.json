{
  "Scaleo": {
    "ApiUrl": "https://api.scaleo.io", 
    // Base URL for the Scaleo API

    "ApiKey": "YOUR_SCALEO_API_KEY" 
    // API key used for authenticating requests to Scaleo
  },

  "BigQuery": {
    "ProjectId": "your-project-id", 
    // Google Cloud project ID where BigQuery is hosted

    "DatasetId": "your_dataset" 
    // BigQuery dataset ID where tables will be created and data inserted
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information" 
      // Default logging level: Information (can be Debug, Warning, Error, etc.)
    }
  }
}